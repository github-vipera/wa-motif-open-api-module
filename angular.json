{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "my-web-admin": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/my-web-admin",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              {
                "input": "node_modules/@progress/kendo-theme-default/scss/all.scss"
              },
              "src/styles.css",
              "src/assets/wc-uikit-kendo.css",
              "src/assets/wc-uikit-toast.css",
              "src/assets/wc-uikit-dashboard.css",
              "node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "my-web-admin:build",
            "proxyConfig": "./proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "my-web-admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "my-web-admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              {
                "input": "node_modules/@progress/kendo-theme-default/scss/all.scss"
              },
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "my-web-admin-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "my-web-admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "my-web-admin:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "wa-motif-open-api-module": {
      "root": "projects/wa-motif-open-api-module",
      "sourceRoot": "projects/wa-motif-open-api-module/src",
      "projectType": "library",
      "prefix": "vp",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/wa-motif-open-api-module/tsconfig.lib.json",
            "project": "projects/wa-motif-open-api-module/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/wa-motif-open-api-module/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wa-motif-open-api-module/src/test.ts",
            "tsConfig": "projects/wa-motif-open-api-module/tsconfig.spec.json",
            "karmaConfig": "projects/wa-motif-open-api-module/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/wa-motif-open-api-module/tsconfig.lib.json",
              "projects/wa-motif-open-api-module/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "wa-motif-open-api-kitchensink-module": {
      "root": "projects/wa-motif-open-api-kitchensink-module",
      "sourceRoot": "projects/wa-motif-open-api-kitchensink-module/src",
      "projectType": "library",
      "prefix": "vp",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/wa-motif-open-api-kitchensink-module/tsconfig.lib.json",
            "project": "projects/wa-motif-open-api-kitchensink-module/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/wa-motif-open-api-kitchensink-module/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wa-motif-open-api-kitchensink-module/src/test.ts",
            "tsConfig": "projects/wa-motif-open-api-kitchensink-module/tsconfig.spec.json",
            "karmaConfig": "projects/wa-motif-open-api-kitchensink-module/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/wa-motif-open-api-kitchensink-module/tsconfig.lib.json",
              "projects/wa-motif-open-api-kitchensink-module/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@wa-motif-open-api/platform-service": {
      "root": "projects/wa-motif-open-api/platform-service",
      "sourceRoot": "projects/wa-motif-open-api/platform-service/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/wa-motif-open-api/platform-service/tsconfig.lib.json",
            "project": "projects/wa-motif-open-api/platform-service/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/wa-motif-open-api/platform-service/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wa-motif-open-api/platform-service/src/test.ts",
            "tsConfig": "projects/wa-motif-open-api/platform-service/tsconfig.spec.json",
            "karmaConfig": "projects/wa-motif-open-api/platform-service/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/wa-motif-open-api/platform-service/tsconfig.lib.json",
              "projects/wa-motif-open-api/platform-service/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@wa-motif-open-api/security-service": {
      "root": "projects/wa-motif-open-api/security-service",
      "sourceRoot": "projects/wa-motif-open-api/security-service/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/wa-motif-open-api/security-service/tsconfig.lib.json",
            "project": "projects/wa-motif-open-api/security-service/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/wa-motif-open-api/security-service/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wa-motif-open-api/security-service/src/test.ts",
            "tsConfig": "projects/wa-motif-open-api/security-service/tsconfig.spec.json",
            "karmaConfig": "projects/wa-motif-open-api/security-service/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/wa-motif-open-api/security-service/tsconfig.lib.json",
              "projects/wa-motif-open-api/security-service/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "my-web-admin"
}